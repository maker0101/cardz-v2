{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    {"src": "/", "dest": "/_shell.html"},
    {"src": "/cart", "dest": "/_shell.html"},
    {"src": "/artist?id=(.*)", "dest": "/_shell.html"},
    {
      "src": "/assets/(.*)\\.(js|css)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/logo-(.*)\\.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
