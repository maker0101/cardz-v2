{
  "name": "ztunes",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"pg,ts,sd\" -c \"#32648c,#7ce645,#ff5515\" \"npm run dev:db\" \"npm run dev:ui\" \"wait-on tcp:localhost:5432 && npx drizzle-kit push --force && npm run seed\"",
    "dev:db": "docker start -a ztunes || docker run --name ztunes -e POSTGRES_PASSWORD=pwd -p 5432:5432 postgres",
    "dev:ui": "vinxi dev",
    "dev:clean": "docker rm -f ztunes",
    "seed": "tsx scripts/seed.ts",
    "build": "vinxi build",
    "start": "vinxi start"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.120.15",
    "@tanstack/react-start": "^1.120.15",
    "drizzle-orm": "^0.44.1",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vinxi": "^0.5.6",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
