{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "ES2022",
    "paths": {
      "@/*": ["./app/*"],
      "@/lib/*": ["./app/frontend/lib/*"]
    }
  }
}
